# Copy this file to .env and fill in your actual values

# API Configuration - REQUIRED
EXPO_PUBLIC_API_BASE_URL=https://your-api-domain.com/api

# App Configuration
EXPO_PUBLIC_APP_NAME=MzigoEgo
EXPO_PUBLIC_APP_VERSION=1.0.0

# Authentication Token Keys
EXPO_PUBLIC_AUTH_TOKEN_KEY=access_token
EXPO_PUBLIC_REFRESH_TOKEN_KEY=refresh_token

# Google Maps API Key - REQUIRED for map functionality
# Get your key from: https://console.developers.google.com/
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Push Notifications - Get from Expo dashboard
EXPO_PUBLIC_PUSH_NOTIFICATION_KEY=your_expo_push_key_here

# Feature Flags
EXPO_PUBLIC_ENABLE_DEV_TOOLS=true
EXPO_PUBLIC_ENABLE_ANALYTICS=false
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=false

# Development Settings
EXPO_PUBLIC_LOG_LEVEL=debug
EXPO_PUBLIC_ENABLE_NETWORK_LOGS=true

# App Identifiers
EXPO_PUBLIC_IOS_BUNDLE_ID=com.your-company.your-app
EXPO_PUBLIC_ANDROID_PACKAGE=com.your-company.your-app

# Deep Linking Scheme
EXPO_PUBLIC_APP_SCHEME=your-app-scheme

# Third-party Services (optional)
# EXPO_PUBLIC_SENTRY_DSN=your_sentry_dsn_here
# EXPO_PUBLIC_ANALYTICS_KEY=your_analytics_key_here

# Cache and Storage
EXPO_PUBLIC_CACHE_VERSION=1.0.0
EXPO_PUBLIC_STORAGE_PREFIX=your_app_prefix_
